# Deployment Configuration
# Set to 'upwork' for Upwork-safe version, omit or set to 'full' for full version
NEXT_PUBLIC_DEPLOYMENT_TYPE=full

# Contact Details Visibility
# Set to 'false' to hide all contact details
NEXT_PUBLIC_SHOW_CONTACT_DETAILS=true

# Individual Contact Methods
NEXT_PUBLIC_SHOW_WHATSAPP=true
NEXT_PUBLIC_SHOW_EMAIL=true
NEXT_PUBLIC_SHOW_PHONE=false
NEXT_PUBLIC_SHOW_DIRECT_CONTACT=true
